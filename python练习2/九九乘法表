#方法一：
for i in range(1,10):
    s=''
    for j in range(1,i+1):
        #s+=str(j)+'*'+str(i)+'='+str(j*i)+' '
        s+='{}*{}={}{}'.format(j,i,j*i,' ')
    print(s)
print'-----------------------------------------'































